<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.system.filemessage.mapper.FileMessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="fileMessageResultMap" type="org.springblade.modules.system.filemessage.entity.FileMessage">
        <id column="id" property="id"/>
        <result column="file_name" property="fileName"/>
        <result column="file_url" property="fileUrl"/>
        <result column="create_user" property="createUser"/>
        <result column="create_dept" property="createDept"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
        <result column="is_deleted" property="isDeleted"/>
    </resultMap>


    <select id="selectFileMessagePage" resultMap="fileMessageResultMap">
        select * from file_message where is_deleted = 0
    </select>

</mapper>
