<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.productInfo.mapper.ProductInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="productInfoResultMap" type="org.springblade.modules.productInfo.entity.ProductInfo">
        <id column="id" property="id"/>
        <result column="product_type_id" property="productTypeId"/>
        <result column="product_model" property="productModel"/>
        <result column="product_name" property="productName"/>
        <result column="product_code" property="productCode"/>
        <result column="service_life" property="serviceLife"/>
        <result column="manufacture_company_id" property="manufactureCompanyId"/>
        <result column="manufacture_company_name" property="manufactureCompanyName"/>
        <result column="design_company_id" property="designCompanyId"/>
        <result column="design_company_name" property="designCompanyName"/>
        <result column="remark" property="remark"/>
        <result column="security_level" property="securityLevel"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="create_dept" property="createDept"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
    </resultMap>


    <select id="selectProductInfoPage" resultMap="productInfoResultMap">
        select * from svc_product_info where is_deleted = 0
    </select>

</mapper>
